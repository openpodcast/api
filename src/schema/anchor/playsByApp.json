{
  "$id": "http://openpodcast.dev/schema/anchor/playsByApp.schema.json",
  "title": "Percent of podcast plays by App and day",
  "type": "object",
  "properties": {
    "provider": {
      "type": "string"
    },
    "version": {
      "type": "number"
    },
    "retrieved": {
      "type": "string"
    },
    "meta": {
      "type": "object",
      "properties": {
        "show": {
          "type": "string"
        },
        "endpoint": {
          "type": "string"
        }
      },
      "required": [
        "show",
        "endpoint"
      ]
    },
    "range": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string"
        },
        "end": {
          "type": "string"
        }
      },
      "required": [
        "start",
        "end"
      ]
    },
    "data": {
      "type": "object",
      "properties": {
        "stationId": {
          "type": "number"
        },
        "kind": {
          "type": "string"
        },
        "parameters": {
          "type": "object",
          "properties": {
            "minValue": {
              "type": "number"
            },
            "timeRange": {
              "type": "array",
              "items": {
                "type": "number"
              }
            },
            "timeInterval": {
              "type": "number"
            }
          },
          "required": [
            "minValue",
            "timeRange",
            "timeInterval"
          ]
        },
        "data": {
          "type": "object",
          "properties": {
            "rows": {
              "type": "array",
              "items": {
                "type": "array",
                "prefixItems": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "number"
                  }
                ],
                "minItems": 2,
                "maxItems": 2
              }
            },
            "columnHeaders": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string"
                  }
                },
                "required": [
                  "name",
                  "type"
                ]
              }
            }
          },
          "required": [
            "rows",
            "columnHeaders"
          ]
        }
      },
      "required": [
        "stationId",
        "kind",
        "parameters",
        "data"
      ]
    }
  },
  "required": [
    "provider",
    "version",
    "retrieved",
    "meta",
    "range",
    "data"
  ]
}
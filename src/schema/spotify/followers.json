{
  "$id": "http://openpodcast.dev/schema/spotify/followers.schema.json",
  "title": "followers of podcast, one count per day",
  "type": "object",
  "properties": {
    "counts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "count": {
            "type": "number"
          },
          "date": {
            "type": "string",
            "format": "date"
          }
        },
        "required": [
          "count",
          "date"
        ]
      }
    }
  },
  "required": [
    "counts"
  ]
}